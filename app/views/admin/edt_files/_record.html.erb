<div class="row">
  <div class="col-md-12">
    <a class="collapse-chevron collapsed" data-toggle="collapse" href="#" data-target="#record-<%= index %>">
      <i class="fa toggle-chevron"></i>
      <%= record %>
    </a>
  </div>
</div>
<div id="record-<%= index %>" class="collapse">
  <div class="row">
    <div class="col-md-offset-1 col-md-11">
      <%= record.class %>
    <table>
    <% record.fields.each do |key, value| %>
      <% unless ["Transaction Identifier", "Record Identification", "Reserved"].include?(key) %>
      <tr>
        <th style="text-align: right; padding-right: 10px;">
          <%= key %>
        </th>
        <td>
          <%= value %>
        </td>
      </tr>
      <% end %>
    <% end %>
    </table>
    </div>
  </div>
</div>
